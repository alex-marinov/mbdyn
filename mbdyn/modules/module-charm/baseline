		const, C,
		const, 0.,
		const, -.5*C*0,
		const, 0.,
		GAUSS_POINTS,
		# naca0012, unsteady, bielawa
		c81, 1 # , unsteady, bielawa
		# c81, interpolated, 2, 1, -1., 1, 1.
		, jacobian, yes
		#, custom output, all, orientation description, orientation vector
		#, output, yes
		#, gauss
		;
