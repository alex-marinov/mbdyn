#/* BEGIN OF OCTAVE SECTION
printf("mboct: embed octave is enabled!\n");

function F = my_force(t)
    global f;
    global F0;
    global tau1;
    global tau2;
    F = (1 - exp(-t/tau1) ) * exp(-t/tau2) * F0 * [ sin(2*pi*f*t); 
               cos(2*pi*f*t); 
               0 ];
endfunction

#*/ END OF OCTAVE SECTION
