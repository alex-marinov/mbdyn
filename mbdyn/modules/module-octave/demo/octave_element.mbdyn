#{
# BEGIN OF MBDYN SECTION

# module load: "libmodule-octave.la";

set: real tau1 = 1e-10;
set: real tau2 = 0.1;

set: real f = 3;
set: real F0 = 130;
set: real r = 15.5;

set: real sx = 7500;
set: real sy = 4800;
set: real sz = 3200;
set: real ks = 0.003;

begin: data;
	problem: initial value; # the default
end: data;

begin: initial value;
	initial time: 0;
	final time: 1;
    time step: 0.002;
	max time step: 0.0005;
    min time step: 0.00001;
	strategy: factor, 0.8, 1, 1.25, 1, 3, 6;
